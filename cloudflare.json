{
	"name": "A Better Browser",
	"description": "Warns users with a simple message at the top of your website if their browser version is too old.",
	"keywords":
	[
		"advocacy",
		"webmaster"
	],
	"version": "1.1.1",
	"contributors":
	[
		{
			"name": "Pavel D.",
			"email": "me@xpaw.ru"
		},
		{
			"name": "Eugene R.",
			"email": "theaqua@thegm.ru"
		}
	],
	"repository":
	{
		"type": "git",
		"url": "https://github.com/xPaw/CF-ABetterBrowser.git"
	},
	"main": "./public/javascripts/abetterbrowser.js",
	"dependencies": { },
	"licenses":
	[
		{
			"type": "CC by-nc-sa",
			"url": "https://creativecommons.org/licenses/by-nc-sa/3.0/"
		}
	],
	"implements": "AMD",
	"scripts": { },
	"config":
	{
		"assets":
		{
			"logos":
			{
				"200px": "./public/images/abetterbrowser-200.png",
				"132px": "./public/images/abetterbrowser-132.png"
			},
			"detail_page":
			[
				"./public/images/example.png"
			]
		},
		"payment": { },
		"details":
		{
			"Category": "Webmaster Tools",
			"Language": "English, Russian, French, Danish, German, Japanese, Turkish",
			"Restrictions": "None",
			"Supporting": "<a href='http://abetterbrowser.org/'>A Better Browser</a>"
		},
		"interface":
		[
			{
				"id": "ie",
				"name": "Internet Explorer",
				"description": "Highest version of IE to display warning (Setting this to 7.0 will display warning in IE7 and lower, but not in IE8)",
				"type": "select",
				"options":
				[
					{
						"label": "6.0",
						"value": "6"
					},
					{
						"label": "7.0",
						"value": "7",
						"selected": true
					},
					{
						"label": "8.0",
						"value": "8"
					},
					{
						"label": "9.0",
						"value": "9"
					},
					{
						"label": "10.0",
						"value": "10"
					}
				]
			}
		]
	}
}
